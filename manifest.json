{
  "manifest_version": 2,
  "name": "Table Search Extension",
  "version": "1.0",
  "description": "Search tables and navigate pages",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    "query.js",
    "scroll.js"
  ]
}
